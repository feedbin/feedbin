<%= render partial: 'shared/update_feeds', locals: {feed_data: @feed_data} %>
feedbin.updateEntries('');
feedbin.updateEntryContent('');
$('body').addClass('nothing-selected').removeClass('feed-selected entry-selected')
$('[data-behavior~=show_tags_form]').replaceWith('<%= j render partial: "shared/buttons/tag", locals: {feed: nil, tags: nil, tag: nil} %>');
$('[data-behavior~=unsubscribe]').replaceWith('<%= j render partial: "shared/buttons/unsubscribe", locals: {feed: nil} %>');
feedbin.updateTitle('<%= j @user.title_with_count %>')
feedbin.disableMarkRead()
feedbin.hideSearchControls()