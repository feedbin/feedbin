<%= render partial: 'shared/update_feeds', locals: {feed_data: @feed_data} %>
feedbin.showPanel(1);
if (feedbin.data.viewMode != 'view_starred') {
$('.feed-id-<%= @subscription.feed_id %>').remove();
}
feedbin.updateEntryContent('');
$('body').addClass('nothing-selected').removeClass('feed-selected entry-selected')
$('[data-behavior~=tags_form]').replaceWith('<%= j render partial: "shared/buttons/tag", locals: {subscription: nil} %>');
$('[data-behavior~=unsubscribe]').replaceWith('<%= j render partial: "shared/buttons/unsubscribe", locals: {subscription: nil} %>');
feedbin.disableMarkRead()
feedbin.hideSearchControls()
$('.modal').modal('hide')
