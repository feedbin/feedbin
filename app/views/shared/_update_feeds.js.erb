feedbin.updateFeeds('<%= j render partial: 'feeds/feeds', locals: {collections: @feed_data[:collections], saved_searches: @feed_data[:saved_searches], tags: @feed_data[:tags], feeds: @feed_data[:feeds]} %>');
feedbin.applyUserTitles();
feedbin.count_data = <%= @feed_data[:count_data].to_json.html_safe %>;
feedbin.resort(<%= @feed_data[:feed_order].to_json.html_safe %>)
feedbin.Counts.get().update(feedbin.count_data);
feedbin.applyCounts(false);
feedbin.draggable();
feedbin.droppable();
$.get(feedbin.data.saved_searches_count_path);
feedbin.faviconColors($(".feeds-column"));
<% if mark_selected? %>
    feedbin.reselect();
<% else %>
    feedbin.selectedSource = null;
    feedbin.selectedTag = null;
<% end %>
