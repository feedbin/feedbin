$("[data-behavior~=subscriptions_list]").replaceWith("<%= j render(Settings::Subscriptions::Shared::List.new(subscriptions: @subscriptions, params: params)) %>");
feedbin.timeago();
feedbin.faviconColors();
<% if params[:q].present? || params[:sort].present? %>
    window.history.replaceState({}, document.title, window.location.pathname + "?<%= j params.permit(:sort, :q).to_query %>");
<% else %>
    window.history.replaceState({}, document.title, window.location.pathname);
<% end %>
