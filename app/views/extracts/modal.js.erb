<% if @content %>
    feedbin.updateDialog("<%= Dialog::ExtractedContent::DIALOG_ID %>", "<%= j render Dialog::ExtractedContent.new(page: @content_info, content: @content) %>");
<% else %>
    feedbin.updateDialog("<%= Dialog::ExtractedContent::DIALOG_ID %>", "<%= j render Dialog::ExtractedContent::Error.new(url: @url) %>");
<% end %>
setTimeout(function() {
  feedbin.formatLinkContents();
}, 351)