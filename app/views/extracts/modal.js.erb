<% if @content %>
    feedbin.updateDialog("<%= Dialog::ExtractedContent::DIALOG_ID %>", "<%= j render Dialog::ExtractedContent.new(page: @content_info, content: @content) %>", true);
<% else %>
    feedbin.updateDialog("<%= Dialog::ExtractedContent::DIALOG_ID %>", "<%= j render Dialog::ExtractedContent::Error.new(url: @url) %>", true);
<% end %>
setTimeout(function() {
  feedbin.formatLinkContents();
}, 351)