<% if params[:subscriptions_hash] && current_user.subscriptions_hash != params[:subscriptions_hash] %>
  feedbin.data.subscriptions_hash = "<%= current_user.subscriptions_hash %>";
  <%= render partial: "shared/update_feeds", locals: {feed_data: @feed_data} %>
<% else %>
  <%= render partial: "shared/update_counts", locals: {feed_data: @feed_data} %>
<% end %>

if ("updateStyles" in feedbin) {
  feedbin.updateStyles("<%= asset_url("tailwind.css") %>");
}
